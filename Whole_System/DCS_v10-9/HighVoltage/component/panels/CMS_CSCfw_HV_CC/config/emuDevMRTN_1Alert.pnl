// s1 is OK
// s2 is ALARM


dps=dpNames(\"*\",\"fwWnrCr_CSC_LV_d\");
//==================================
for(i=1;i<=dynlen(dps);i++){

  if(strpos(dps[i],\"DimBroker\")>=0)continue;  
DebugTN(\"==============\"+dps[i]);
    
  limits_s=makeDynString(\"1\"); // that gives an alarm if the value >= 1

 DebugTN(\">>>>> alerting .....\"+dps[i]+\" \"+dynlen(cr_er_dpe));
  

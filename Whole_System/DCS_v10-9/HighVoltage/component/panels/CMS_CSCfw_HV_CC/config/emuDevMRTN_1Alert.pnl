   dpSet(mudcsRemoveSystem(dps[i])+\".\"+\":_alert_hdl.._active\", true);
  }
  else{
   fwAlertConfig_deleteSummary( mudcsRemoveSystem(dps[i])+\".\",exceptionInfo);        
  }

dyn_string s1;

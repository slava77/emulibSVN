"main()
{
// Local Variables
// ---------------
	int i;
	
	unsigned uSyncInterval;
	unsigned uNodeGuard;
	unsigned uManagement;
	
	string sBuses = $sBusNames;
	string sManagement;
	
	dyn_string dsBusList;

// Executable Code
// ---------------
	// Get a list of the buses
	fwGeneral_stringToDynString(sBuses, dsBusList);

	// Loop through all buses
	for (i = 1; i <= dynlen(dsBusList); i++) {
	
		// Get buses parameters from the DP
		dpGet(dsBusList[i] + \".syncInterval\", uSyncInterval,
					dsBusList[i] + \".nodeGuardInterval\", uNodeGuard,
					dsBusList[i] + \".management\", uManagement);
					
		// Set the management text
		switch(uManagement) {
			case 1:
				sManagement = \"STARTED\";
				break;
			case 2:
				sManagement = \"STOPPED\";
				break;
			default:
				sManagement = \"RESET\";
				break;
		}
		
		// Enter the data into the table
		this.updateLine(1, 	\"BusName\", dsBusList[i],
												\"SyncInterval\", uSyncInterval,
												\"NodeGuarding\", uNodeGuard,
												\"Management\", sManagement);
 	}
 	
 	// Return to calling routine
 	return;
}" 0

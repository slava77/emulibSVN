LANG:1 4 Read
"main()
{
// Local Variables
// ---------------
  unsigned uState;
  unsigned uInitHigh;

  string sElmbName;
  string sMessage;
  
  anytype aValue;

  dyn_string dsExceptionInfo;
			
// Executable Code
// ---------------
  // Disable certain controls while the read takes place
  this.enabled = false;
  cmdWrite.enabled = false;

  // Give some information to the user
  txtMessage.text = \"Reading Digital Output Settings. Please wait...\";
  fwOpenProgressBar(\"Reading Digital Output Settings\", \"In progress. Please wait...\", 1);

  // Get the parent (ELMB) that this config comes from
  fwDevice_getParent($sDpName, sElmbName, dsExceptionInfo);			
	
  // Get current state of ELMB
  dpGet(sElmbName + \".state.value\", uState);
			
  // Set node to Pre-Operational:
  dpSetWait(sElmbName + \".management\", 128);

  // Query the value
  fwElmb_elementSQ($sDpName + \".initHigh.read\", 2, aValue, dsExceptionInfo);
  if (dynlen(dsExceptionInfo) == 0) {
    uInitHigh = aValue;
    spnLowHigh.enabled = true;
    switch(uInitHigh) {
      case 0:
        spnLowHigh.number = 0;
        break;
      case 1:
        spnLowHigh.number = 1;
      break;
    }
  } else {
    spnLowHigh.enabled = false;
  }
  fwElmbUser_updateFirmwareInfo(sElmbName);

  // Set the ELMB back to it's original state, but only if it was stopped or operational.
  // Otherwise, leave it as preoperational
  if ((uState == 0x84) || (uState == 0x04))
    dpSet(sElmbName + \".management\", 2);
  else if ((uState  == 0x85) || (uState == 0x05))
    dpSet(sElmbName + \".management\", 1);

  // Check for errors
  if (dynlen(dsExceptionInfo) > 0) {
    fwCloseProgressBar(\"Problems reading DI settings\");
    txtMessage.text = \"Info: Problems reading DO settings\";
    fwExceptionHandling_display(dsExceptionInfo);
  } else {
    fwCloseProgressBar(\"DO settings successfully read\");
    txtMessage.text = \"Info: DO settings successfully read\";
  }
		
  // Re-enable controls
  this.enabled = true;
  cmdWrite.enabled = true;

  // Return to calling routine
  return;
}

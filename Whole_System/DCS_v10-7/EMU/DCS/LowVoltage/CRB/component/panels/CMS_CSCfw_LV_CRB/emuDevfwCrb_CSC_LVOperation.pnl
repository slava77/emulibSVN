test_dyn=strsplit(state,\" \");
test_dyn2=strsplit(test_dyn[2],\"h\");

chn=test_dyn2[2];
chn_base0=chn-1;

dp_frag=makeDynString(\"ai_\"+CSC_fwCAN1_g_LIST_5V[chn],
                      \"ai_\"+CSC_fwCAN1_g_LIST_3V1[chn],\"ai_\"+CSC_fwCAN1_g_LIST_3V2[chn]);

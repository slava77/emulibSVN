dmb_t_init(){
 int i, index;
 dyn_int off_channels;
 
 for(i=1;i<=dynlen(chamber_fsms);i++){
    dpGet(chamber_fsms[i]+\".off_channels\",off_channels);
    if(dynContains(off_channels,7)){
     setValue(\"t_\"+i,\"enabled\",false);

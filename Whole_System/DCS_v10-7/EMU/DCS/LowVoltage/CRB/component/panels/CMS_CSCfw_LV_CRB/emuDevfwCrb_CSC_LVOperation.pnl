     setValue(\"t_\"+i,\"enabled\",true);
     setValue(\"te_\"+i,\"backCol\",\"{0,153,153}\");        
    } 
 }
  
  

}
//===========================================================
dmb_t_exclude(){
  
string state;
dyn_string ds1, ds2;
int chn, chn_base0;
int i, index;
dyn_int off_channels;

//getValue(\"\",\"text\",state);

string box_name;
bool isEnabled;

getValue(\"\",\"name\",box_name);
getValue(\"\",\"state\",0,isEnabled);
  
ds1=strsplit(box_name,\"_\");  
chn=ds1[2];  

if(isEnabled){
  
  dpGet(chamber_fsms[chn]+\".off_channels\",off_channels);
  if((index=dynContains(off_channels,7)))dynRemove(off_channels,index);
  dpSet(chamber_fsms[chn]+\".off_channels\",off_channels);
  setValue(\"t_\"+chn,\"enabled\",true);
  setValue(\"\",\"backCol\",\"{0,153,153}\");  
}  
else{
  dpGet(chamber_fsms[chn]+\".off_channels\",off_channels);
  if(!(index=dynContains(off_channels,7)))dynAppend(off_channels,7);
  dpSet(chamber_fsms[chn]+\".off_channels\",off_channels);
  setValue(\"t_\"+chn,\"enabled\",false);
  setValue(\"\",\"backCol\",\"{202,202,202}\");  
}


}

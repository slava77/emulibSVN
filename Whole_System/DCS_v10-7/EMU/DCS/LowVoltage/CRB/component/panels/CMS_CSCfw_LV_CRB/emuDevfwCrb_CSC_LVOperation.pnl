//==================================
exclude(int isexclude, string chn_text, dyn_string dp_frag){

int chn=chn_text;

dyn_int off_channels;
int pos;

dpGet(DpName+\".off_channels\",off_channels);

 if(isexclude){
  if(dynContains(off_channels,chn)==0)dynAppend(off_channels,chn);
 }
 else{
  pos=dynContains(off_channels,chn);
  if(pos)dynRemove(off_channels,pos);

 }
 
dpSetWait(DpName+\".off_channels\",off_channels);
  alert_reconfig(dp_frag, isexclude);   
 
}
//====================================
alert_reconfig(dyn_string dp_frag, int isexclude){

bool configExists;
dyn_string alertTexts;
dyn_string ilist, ilist_new;
string alertPanel;
dyn_string alertPanelParameters;
string alertHelp;
bool isActive;
dyn_string exceptionInfo;
int pos;

int i,j;
dyn_int dplist;
dyn_string dplist_text, ds1;
int current_status;
 dpGet(DpName+\".status\",current_status);


fwAlertConfig_getSummary(DpName+\".\",configExists,alertTexts,ilist,
	alertPanel,alertPanelParameters,alertHelp,isActive,exceptionInfo);

//DebugTN(dp_frag+\"=========\");

for(i=1;i<=dynlen(ilist);i++){ 
  for(j=1;j<=dynlen(dp_frag);j++){
 if(strpos(ilist[i],dp_frag[j])>=0){
  dynAppend(dplist,i);
 } // j
//  mudcsLvDebug(i);
 }
}
//DebugTN(dplist+\"+++=========\");

ilist_new=ilist;


if(isexclude){
 dynClear(ilist);
 for(i=1;i<=dynlen(ilist_new);i++){
  if(dynContains(dplist,i) == 0)dynAppend(ilist,ilist_new[i]);
 }
 
// DebugTN(ilist+\"---=========\");
 
 if(current_status != 0 )dpSet(DpName+\".status\",2);
}
else{
 
 
// dplist_text=dpNames(DpName+\".**\"+dp_frag+\"**\");
   for(j=1;j<=dynlen(dp_frag);j++){ 
    ds1=dpNames(\"*ELMB_\"+pnl_coord+\"*\"+dp_frag[j],\"FwElmbAi\");
//    DebugTN(\"*ELMB_\"+pnl_coord+\"*\"+dp_frag[j]);
//    DebugTN(ds1);
          dynAppend(dplist_text,ds1); 
   } // for j
   
 for(i=1;i<=dynlen(dplist_text);i++){
  dynAppend(ilist,dplist_text[i]+\".value\"); 
 }
 if(current_status != 0)dpSet(DpName+\".status\",2);
}

//=====

  for(i=1;i<=dynlen(ilist);i++){
   DebugTN(ilist[i]); 
  } 
							
 fwAlertConfig_createSummary(DpName+\".\",makeDynString(\"\",\"\") , ilist ,\"\", 
                              makeDynString(),\"\",exceptionInfo);
//======

}

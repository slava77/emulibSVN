dp_frag=makeDynString(\"ai_\"+CSC_fwCAN1_g_LIST_MPC[1],\"ai_\"+CSC_fwCAN1_g_LIST_MPC[2]);}
else if(test_dyn[2]==\"cc\"){test_dyn2[2]=\"11\";
dp_frag=makeDynString(\"ai_\"+CSC_fwCAN1_g_LIST_CC[1],\"ai_\"+CSC_fwCAN1_g_LIST_CC[2]);}
else if(test_dyn[2]==\"1.5\"){test_dyn2[2]=\"12\";
dp_frag=makeDynString(\"ai_\"+CSC_fwCAN1_g_LIST_15V[1],\"ai_\"+CSC_fwCAN1_g_LIST_15V[2]);}

chn=test_dyn2[2];
if(chn<1 || chn>12){
 mudcsLvDebug(\"wrong format\\n righ format is:\\nexclude ch03\");
 return;
}

if(strpos(state,\"exclude\")>=0)exclude(1,test_dyn2[2],dp_frag);
else if(strpos(state,\"include\")>=0)exclude(0,test_dyn2[2],dp_frag);
else {
 mudcsLvDebug(\"wrong format\\n righ format is:\\nexclude ch03\");
 return;
}



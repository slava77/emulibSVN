E "search()
{
  lblResultCount.text = \"searching...\";
  resultsTable.deleteSelection();
  resultsTable.deleteAllLines();
   
  string searchString = editSearch.text;
  dyn_string types = dpTypes(\"*\");
  dyn_dyn_anytype data;
  dpQuery(\"SELECT '_original.._value' FROM '*' WHERE _LEAF\", data);
  emu_info(\"Total DPEs selected: \" + dynlen(data));
  emu_info(\"Searching...\");
  dyn_dyn_string results;
  for (int i=1; i <= dynlen(data); i++){
//     string value = dpValToString(dpSubStr(data[i][1], DPSUB_SYS_DP_EL), data[i][2]);
    string value;
    if ((getType(data[i][2]) != BLOB_VAR) && 
        (getType(data[i][2]) != BIT32_VAR) &&
        (getType(data[i][2]) != DYN_BIT32_VAR) &&
        (getType(data[i][2]) != DYN_BIT32_VAR) &&
        (getType(data[i][2]) != DYN_DYN_BIT32_VAR) &&
        (getType(data[i][2]) != DYN_DYN_BIT32_VAR)) {
      
      value = (string) data[i][2];
    }
    
    if (strpos(value, searchString) >= 0) {
      string dp = dpSubStr(data[i][1], DPSUB_DP_EL_CONF_DET_ATT);
      string type = dpTypeName(dp);
      dyn_string resultLine = makeDynString(type, dp, value);
      dynAppend(results, resultLine);//makeDynString(data[i][1], data[i][2]));
    }
  }
  emu_info(\"Result count: \" + dynlen(results));
  lblResultCount.text = dynlen(results);
  for (int i=1; i <= dynlen(results); i++) {
    resultsTable.appendLine(\"type\", results[i][1], \"DPE\", results[i][2], \"value\", results[i][3]);
  }
}

printMap(dyn_dyn_anytype map) {
  emu_info(\"Printing map:\");
  for (int i=1; i <= dynlen(map); i++) {
    string line = \"\";
    for (int j=1; j <= dynlen(map[i]); j++) {
      line += \"\\t\" + map[i][j] + \"\\t|\";
    }
    emu_info(line);
  }

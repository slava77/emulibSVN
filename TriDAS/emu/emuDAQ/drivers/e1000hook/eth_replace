cp $XDAQ_ROOT/emu/emuDAQ/drivers/e1000hook/eth_hook_4_nobigphysxxx/eth_hook_4.ko $KDIR/drivers/net/eth_hook_4.ko
cp $XDAQ_ROOT/emu/emuDAQ/drivers/e1000hook/eth_hook_5_nobigphysxxx/eth_hook_5.ko $KDIR/drivers/net/eth_hook_5.ko
/sbin/depmod -a
rmmod e1000h

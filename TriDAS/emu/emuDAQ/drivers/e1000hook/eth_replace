chmod 755 $XDAQ_ROOT/emu/emuDAQ/drivers/e1000hook/eth_hook_5_nobigphysxxx/eth_hook_5.ko
export KDIR=/lib/modules/$(uname -r)/kernel
echo $KDIR
cp $XDAQ_ROOT/emu/emuDAQ/drivers/e1000hook/e1000-7.0.39/src/e1000h.ko $KDIR/drivers/net/e1000h/e1000h.ko
cp $XDAQ_ROOT/emu/emuDAQ/drivers/e1000hook/e1000-7.0.39/src/e1000h.ko $KDIR/drivers/net/e1000h.ko
cp $XDAQ_ROOT/emu/emuDAQ/drivers/e1000hook/eth_hook_2_nobigphysxxx/eth_hook_2.ko $KDIR/drivers/net/eth_hook_2.ko

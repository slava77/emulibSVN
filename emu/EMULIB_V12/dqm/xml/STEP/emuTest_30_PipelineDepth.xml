<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<Booking xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
	<Config>
		<ExpectedEvents>1</ExpectedEvents>
		<Description>Pipeline Depth Test</Description>
		<PD_min>64</PD_min>
		<PD_max>73</PD_max>
        </Config>

		<TestResult>
                <Test>30_PipelineDepth</Test>
                <Type>h1</Type>
                <Title>V00 - Format Errors Frequency</Title>
                <Prefix>CSC</Prefix>
                <Name>V00</Name>
                <XMin>0zz</XMin>
                <XMax>20</XMax>
                <XBins>20</XBins>
                <SetOption>texthbar1</SetOption>
		<YTitle>Frequency (%)</YTitle>
		<SetOptStat>e</SetOptStat> 
		<SetXLabels>1='DMB Trailer Missing'|2='DMB Header Missing'|3='ALCT Trailer Missing'|4='ALCT Header Missing'|5='ALCT Word Count Error'|6='ALCT CRC Error'|7='ALCT Trailer Bit Error'|8='TMB Trailer Missing'|9='TMB Header Missing'|10='TMB Word Count Error'|11='TMB CRC Error'|12='CFEB Word Count Per Sample Error'|13='CFEB Sample Count Error'|14='CFEB CRC Error'|15='DDU Event Size Limit Error'|16='C-Words'|17='ALCT DAV Error'|18='TMB DAV Error'|19='CFEB DAV Error'|20='DMB Active Error'|</SetXLabels>
		<SetLabelSizeX>0.02</SetLabelSizeX>
		<SetLabelSizeY>0.03</SetLabelSizeY>
		<SetNdivisionsX>20</SetNdivisionsX>                
		<SetLeftMargin>0.3</SetLeftMargin>
        </TestResult>

		<TestResult>
                <Test>30_PipelineDepth</Test>
                <Type>h2</Type>
                <Title>V01 - PipelineDepth vs PeakTimeBin</Title>
                <Prefix>CSC</Prefix>
                <Name>V01</Name>
                <XMin>62.5</XMin>
                <XMax>72.5</XMax>
                <XBins>10</XBins>
								<XTitle>PipelineDepth</XTitle>
                <YMin>-0.5</YMin>
                <YMax>7.5</YMax>
                <YBins>8</YBins>
								<YTitle>TimeBin</YTitle>
                <SetOption>colz</SetOption>
		<SetLabelSizeX>0.02</SetLabelSizeX>
		<SetLabelSizeY>0.03</SetLabelSizeY>
		<SetLeftMargin>0.3</SetLeftMargin>
        </TestResult>
								
	<TestResult>
                <Test>30_PipelineDepth</Test>
                <Type>h2</Type>
                <Title>V02 - PD vs. Min Time Bin</Title>
                <Prefix>CSC</Prefix>
                <Name>V02</Name>
                <XMin>62.5</XMin>
                <XMax>72.5</XMax>
                <XBins>10</XBins>
								<XTitle>PipelineDepth</XTitle>
                <YMin>0</YMin>
                <YMax>8</YMax>
                <YBins>8</YBins>
                <YTitle>Minimum Time Bin</YTitle>
                <SetOption>colz</SetOption>
		<SetLabelSizeX>0.02</SetLabelSizeX>
		<SetLabelSizeY>0.03</SetLabelSizeY>
		<SetLeftMargin>0.3</SetLeftMargin>
        </TestResult>

	<TestResult>
                <Test>30_PipelineDepth</Test>
                <Type>h2</Type>
                <Title>V03 - PD vs. Max over pedestal</Title>
                <Prefix>CSC</Prefix>
                <Name>V03</Name>
                <XMin>62.5</XMin>
                <XMax>72.5</XMax>
                <XBins>10</XBins>
								<XTitle>PipelineDepth</XTitle>
                <YMin>0</YMin>
                <YMax>1000</YMax>
                <YBins>100</YBins>
                <YTitle>Q_{max}-Q_{ped} (ADC)</YTitle>
                <SetOption>colz</SetOption>
		<SetLabelSizeX>0.02</SetLabelSizeX>
		<SetLabelSizeY>0.03</SetLabelSizeY>
		<SetLeftMargin>0.3</SetLeftMargin>
        </TestResult>

	<TestResult>
                <Test>30_PipelineDepth</Test>
                <Type>h2</Type>
                <Title>V04 - PD vs. Pedestal</Title>
                <Prefix>CSC</Prefix>
                <Name>V04</Name>
                <XMin>62.5</XMin>
                <XMax>72.5</XMax>
                <XBins>10</XBins>
								<XTitle>PipelineDepth</XTitle>
                <YMin>60</YMin>
                <YMax>1400</YMax>
                <YBins>100</YBins>
                <YTitle>Pedestal (ADC)</YTitle>
                <SetOption>colz</SetOption>
		<SetLabelSizeX>0.02</SetLabelSizeX>
		<SetLabelSizeY>0.03</SetLabelSizeY>
		<SetLeftMargin>0.3</SetLeftMargin>
        </TestResult>


	<TestResult>
                <Test>30_PipelineDepth</Test>
                <Type>h2</Type>
                <Title>V05 - PD vs. Max Strip</Title>
                <Prefix>CSC</Prefix>
                <Name>V05</Name>
                <XMin>62.5</XMin>
                <XMax>72.5</XMax>
                <XBins>10</XBins>
								<XTitle>PipelineDepth</XTitle>
                <YMin>0.5</YMin>
                <YMax>16.5</YMax>
                <YBins>16</YBins>
                <YTitle>Strip with Largest Charge</YTitle>
                <SetOption>colz</SetOption>
		<SetLabelSizeX>0.02</SetLabelSizeX>
		<SetLabelSizeY>0.03</SetLabelSizeY>
		<SetLeftMargin>0.3</SetLeftMargin>
        </TestResult>

</Booking>

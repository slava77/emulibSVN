E "const string systemName = \"cms_csc_dcs_02\";
int settingCounter = 0;
dyn_string channelDps;
dyn_string tableColumns;
bool syncWithOpcPoll = false;
bool waitForVMonWhileBouncing = false;
bool cancelVSet = false;
bool stopBouncing = false;
bool bouncing = false;
bool settingVoltage = false;
int setVoltageOnOpcUpdate = -1;
mapping vMons;

int vMax, vMin;

void showError(string msg) {
  lblError.visible = true;
  string text = lblError.text();
  lblError.text = text + \"\\n\" + msg;
}

void setSettingCounter(int count) {
  settingCounter = count;
  lblSettingCounter.text = settingCounter;
}

void increaseSettingCounter() {
  settingCounter++;
  lblSettingCounter.text = settingCounter;
}

void setVoltage(int voltage, bool callFromOpcUpdateCB = false) {
  cancelVSet = false;
  if (!bouncing) {
    controlsAvailable(false, true, false);
  }
  if (callFromOpcUpdateCB) {
    lblStatus.text = \"OPC updated, waiting \" + delayAfterOpcPoll.text + \"ms to set voltage to \" + voltage + \"V\";
    delay(0, delayAfterOpcPoll.text);
  }
  if (syncWithOpcPoll && !callFromOpcUpdateCB) {
    setVoltageOnOpcUpdate = voltage;
    lblStatus.text = \"Waiting for OPC update to set voltage to \" + voltage + \"V\";
    return;
  }
  if (cancelVSet) { return; settingVoltage = false; }
  settingVoltage = true;
  lblStatus.text = \"Setting voltage to \" + voltage + \"V with delay of \" + 
                   spinDelay.text + \"ms between channels...\";
  DebugTN(\"Setting all channels to voltage: \" + voltage + \"V\");
  for (int i=1; i <= dynlen(channelDps); i++) {
    if (cancelVSet) { return; settingVoltage = false; }
    dpSetWait(channelDps[i] + \".settings.v0\", voltage);
    delay(0, spinDelay.text);
  }
  increaseSettingCounter();  
  if (!bouncing) {
    controlsAvailable(true);
    lblStatus.text = \"idle\";
  }
  settingVoltage = false;
}

void startBouncing() {
  stopBouncing = false;
  bouncing = true;
  controlsAvailable(false, true, true);
  while(!stopBouncing) {
    setVoltage(spinV1.text);
    while (settingVoltage || (syncWithOpcPoll && (setVoltageOnOpcUpdate != -1))) { // wait until voltage is set if it's asynchronous
      delay(0, 100);
      if (stopBouncing) { break; }
    }
    lblStatus.text = \"Waiting for \" + spinDelayBetweenBounces.text + \"s before next round (to \" + spinV2.text + \"V)\";
    if (stopBouncing) { break; }
    delay(spinDelayBetweenBounces.text);
    if (waitForVMonWhileBouncing) {
      if (stopBouncing) { break; }
      waitForVMonToSettle(spinV1.text);
    }
    if (stopBouncing) { break; }
    setVoltage(spinV2.text);
    while (settingVoltage || (syncWithOpcPoll && (setVoltageOnOpcUpdate != -1))) { // wait until voltage is set if it's asynchronous
      delay(0, 100);
      if (stopBouncing) { break; }
    }
    lblStatus.text = \"Waiting for \" + spinDelayBetweenBounces.text + \"s before next round (to \" + spinV1.text + \"V)\";
    if (stopBouncing) { break; }
    delay(spinDelayBetweenBounces.text);
    if (waitForVMonWhileBouncing) {
      if (stopBouncing) { break; }
      waitForVMonToSettle(spinV2.text);
    }
  }
  bouncing = false;
  controlsAvailable(true);
  lblStatus.text = \"idle\";
}

void waitForVMonToSettle(int voltage) {
  lblStatus.text = \"Checking vMon before continuing\";
  while (!stopBouncing) {
    delay(2);
    if (vMax - vMin <= 30) {
      int absDiffFromVMin = voltage - vMin;
      if (absDiffFromVMin < 0) { absDiffFromVMin *= -1; }
      int absDiffFromVMax = voltage - vMax;
      if (absDiffFromVMax < 0) { absDiffFromVMax *= -1; }
      if ((absDiffFromVMin < 5) && (absDiffFromVMax < 5)) {
        break;
      } else {
        lblStatus.text = \"Absolute difference of vMin or vMax from the voltage setting is too big, waiting...\";
      }
    } else {
      lblStatus.text = \"Difference between vMin and vMax is too big, waiting...\";
    }
  }
}

string _formatTime(time t) {
  return formatTime(\"%H:%M:%S\", t, \".%03d\");
}

void opcServerUpdatedCB(string dp, time timestamp) {
  if (syncWithOpcPoll && (setVoltageOnOpcUpdate != -1)) {
    string voltage = setVoltageOnOpcUpdate;
    settingVoltage = true;
    setVoltageOnOpcUpdate = -1;
    setVoltage(voltage, true);
  }
  tableOpcUpdates.insertLineN(0);
  tableOpcUpdates.cellValueRC(0, \"update_time\", _formatTime(timestamp));
  if (tableOpcUpdates.lineCount() > 50) {
    tableOpcUpdates.deleteLineN(tableOpcUpdates.lineCount() - 1);
  }
}

void channelDataUpdatedCB(dyn_string dpes, dyn_anytype data) {
  int lineIdx = dynContains(channelDps, dpSubStr(dpes[1], DPSUB_SYS_DP));
  if (lineIdx <= 0) {
    showError(\"channel\" + dpSubStr(dpes[1], DPSUB_SYS_DP) + \" was not found\");
    return;
  }
  
  lineIdx--;
  for (int i=1; i <= dynlen(data); i++) {
    string columnName = tableColumns[i];
    string dataStr;
    if (strpos(columnName, \"timestamp\") >= 0) {
      dataStr = _formatTime(data[i]);
    } else {
      dataStr = data[i];
    }
    tableChannels.cellValueRC(lineIdx, columnName, dataStr);
    if (columnName == \"vmon\") {
      updateVoltage(dpSubStr(dpes[1], DPSUB_SYS_DP), (int)data[i]);
    }
  }
}

void updateVoltage(string dp, int voltage) {
  if (mappingHasKey(vMons, dp) && (vMons[dp] == voltage)) { return; }
  vMons[dp] = voltage;
  int newVMax, newVMin;
  for (int i=1; i <= mappinglen(vMons); i++) {
    int v = mappingGetValue(vMons, i);
    if (v > newVMax) {
      newVMax = v;
    }
    if (v < newVMin) {
      newVMin = v;
    }
  }
  
  vMin = newVMin;
  vMax = newVMax;
  lblMinV.text = vMin;
  lblMaxV.text = vMax;
}

void controlsAvailable(bool available, bool cancelVsetAvailable = true, bool stopBouncingAvailable = true) {
  btnSetV1.enabled = available;
  btnSetV2.enabled = available;
  btnResetCounter.enabled = available;
  btnStartBouncing.enabled = available;
  btnCancelVSet.enabled = cancelVsetAvailable;
  btnStopBouncing.enabled = stopBouncingAvailable;
}" 0

"main()
{
  string dp = emuui_getSessionDp(\"valueMonitor\");
  string connectionType, mode;
  dpGet(dp + \".connectionType\", connectionType);
  dpGet(dp + \".mode\", mode);
  
  int index = emu_dynDynContains(valueMonitorModes, makeDynString(connectionType, mode));
  if (index > 0) {
    this.number(index - 1);

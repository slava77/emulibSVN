  ds1=dpNames(\"*Crate\"+individual_ids[i]+\"/Channel\"+s1,\"FwWienerMarathonChannel\");
   ///////////////////dpSet(ds1[1]+\".Settings.\"+listOfElement[j],individual_vals[i]); 
  DebugTN(\"Crate\"+individual_ids[i]+\"/Channel\"+s1);
  DebugTN(ds1[1]+\".Settings.\"+listOfElement[j]+\"     \"+individual_vals[i]); 
  }
mudcsLvDebug(\"done\");  
}

       else if(igr==2) setting_channel=settings_group2[ich];
       else if(igr==3) setting_channel=settings_group3[ich];
       else if(igr==4) setting_channel=settings_group4[ich];
       else if(igr==5) setting_channel=settings_group5[ich];
       else if(igr==6) setting_channel=settings_group6[ich];
       else if(igr==7) setting_channel=settings_group7[ich];
       else if(igr==8) setting_channel=settings_group8[ich];
       else if(igr==9) setting_channel=settings_group9[ich];
       else if(igr==10) setting_channel=settings_group10[ich];
       found=true;    
      }
    } // for igr
  } // for j
    if(!found)continue;
     
  mudcsMrtnSettings(channel_dps[i], setting_channel);

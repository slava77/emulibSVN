class: $FWPART_CSC_LV_TREE_NODES_FwDevMode_CLASS
    state: READY
        action: Disable(Device)
            remove &VAL_OF_Device from $FWPART_CSC_LV_TREE_NODES_FWSETSTATES
            remove &VAL_OF_Device from $FWPART_CSC_LV_TREE_NODES_FWSETACTIONS
            move_to READY
        action: Enable(Device)
            insert &VAL_OF_Device in $FWPART_CSC_LV_TREE_NODES_FWSETSTATES
            insert &VAL_OF_Device in $FWPART_CSC_LV_TREE_NODES_FWSETACTIONS
            move_to READY

object: $FWPART_CSC_LV_TREE_NODES_FWDM is_of_class $FWPART_CSC_LV_TREE_NODES_FwDevMode_CLASS


class: $FWPART_FwWienerMarathonChannel_FwDevMode_CLASS
    state: READY
        action: Disable(Device)
            remove &VAL_OF_Device from $FWPART_FWWIENERMARATHONCHANNEL_FWSETSTATES
            remove &VAL_OF_Device from $FWPART_FWWIENERMARATHONCHANNEL_FWSETACTIONS
            move_to READY
        action: Enable(Device)
            insert &VAL_OF_Device in $FWPART_FWWIENERMARATHONCHANNEL_FWSETSTATES
            insert &VAL_OF_Device in $FWPART_FWWIENERMARATHONCHANNEL_FWSETACTIONS
            move_to READY

object: $FWPART_FwWienerMarathonChannel_FWDM is_of_class $FWPART_FwWienerMarathonChannel_FwDevMode_CLASS


class: $FWPART_FwWienerMarathon_FwDevMode_CLASS
    state: READY
        action: Disable(Device)
            remove &VAL_OF_Device from $FWPART_FWWIENERMARATHON_FWSETSTATES
            remove &VAL_OF_Device from $FWPART_FWWIENERMARATHON_FWSETACTIONS
            move_to READY
        action: Enable(Device)
            insert &VAL_OF_Device in $FWPART_FWWIENERMARATHON_FWSETSTATES
            insert &VAL_OF_Device in $FWPART_FWWIENERMARATHON_FWSETACTIONS
            move_to READY

object: $FWPART_FwWienerMarathon_FWDM is_of_class $FWPART_FwWienerMarathon_FwDevMode_CLASS


E "// Panel Usage
// ===========
// This reference panel does not use a connection to a datapoint. Instead, the setting and getting
// of values is via setting values into or reading from certain objects in the panel.
//
// To set an actual value to the object, enter the value into the 'txtInputValue' field
// e.g.
//  setValue(\"<PanelRefName>.txtInputValue\", \"text\", uValue);
//
// To set the control as invalid, enter the value \\\"Invalid\\\" into the 'txtInputValue' field
// e.g.
//  setValue(\"<PanelRefName>.txtInputValue\", \"text\", \"Invalid\");
//
// To get the current value of the object, 'request' an update by first entering an empty
// string into the 'txtInputValue' field
// e.g.
//  setValue(\"<PanelRefName>.txtInputValue\", \"text\", \"\");
//
// The actual value may then be obtain by reading the 'txtOutputValue' field and checking
// for validity. If valid, then the string value obtained my then be entered into the
// desired variable type
// e.g.
//  getValue(\"<PanelRefName>.txtOutputValue\", \"text\", sValue);
//  if (sValue != \"Invalid\") {
//    uValue = sValue;

void setState(string sObject, bool bValue)
{
  setValue(sObject, \"enabled\", true);
  setValue(sObject, \"toggleState\", bValue);
  if (bValue)
    setValue(sObject, \"text\", \"1\");
  else
    setValue(sObject, \"text\", \"0\");
  setOutputValue();
}

void setInvalid(string sObject)
{
  setValue(sObject, \"enabled\", false);
  setValue(sObject, \"toggleState\", false);
  setValue(sObject, \"text\", \"X\");
}

void setAllInvalid()
{
  int i;
  for (i = 0; i < 8; i++)
    setInvalid(\"tglBit\" + i);
}

void setOutputValue()
{
  bool bValue;
  bool bValid = true;
  int i;
  int iOutputValue = 0;
  for (i = 0; (i < 8) && bValid; i++) {
    getValue(\"tglBit\" + i, \"enabled\", bValid);
    if (bValid) {
      getValue(\"tglBit\" + i, \"toggleState\", bValue);
      iOutputValue += bValue ? (1 << i) : 0;
    }
  }
  if (bValid)
    txtOutputValue.text = iOutputValue;
  else
    txtOutputValue.text = \"Invalid\";
}

LANG:1 18 Create Config File
"main()
{
// Local Variables
// ---------------
  bool bUseNodeGuarding;

  string sFilename;

  dyn_float df;
	
  dyn_string dsBuses;
  dyn_string ds;
  dyn_string dsExceptionInfo;
	
// Executable Code
// ---------------
  // Get information from panel controls
  dsBuses = lstSelectedBusNames.items();
  sFilename = txtFilename.text;
  bUseNodeGuarding = chbNodeGuarding.state(0);
	
  // The INIT section is created from this panel (to be changed)
  ChildPanelOnCentralReturn(\"fwElmb/fwElmbListBuses.pnl\",
                            \"Buses Features\",
                            makeDynString(\"$sBusNames:\" + dsBuses),
                            df, ds);
														
  // Check if cancel was pressed
  if (df[1] > 0.0) {

    // Disable the create button
    this.enabled = false;
	
    // Call function to create the configuration file
    fwElmbUser_createOPCFile(dsBuses,
                             sFilename,
                             bUseNodeGuarding,
                             dsExceptionInfo);

    // Check for errors
    if (dynlen(dsExceptionInfo) > 0) {
      fwExceptionHandling_display(dsExceptionInfo);
    } else {
      ChildPanelOnCentral(\"vision/MessageInfo1\",
                          \"Success\",
                          makeDynString(\"Config file successfully created.\\nHave fun!!!\", \"OK\"));
    }
  }
	
  // Re-enable the button
  this.enabled = true;
	
  // Return to calling routine
  return;
}

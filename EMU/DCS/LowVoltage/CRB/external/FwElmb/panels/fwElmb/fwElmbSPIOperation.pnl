E"void modReadSPIValue(	string sDpElement,
											string sControlName,
											bool bByteData = false)
{
// Local Variables
// ---------------
	int iResult;
	
	string sUnsigned;
	string sHex;
	
	dyn_string dsExceptionInfo;
	
// Executable Code
// ---------------
	// Call function to get required data
	iResult = fwElmbUser_readSPIValue($sDpName,
																		sDpElement,
																		dsExceptionInfo);
																		
	// Check for errors
	if (dynlen(dsExceptionInfo) > 0) {
		fwExceptionHandling_display(dsExceptionInfo);
		setValue(sControlName, \"text\", \"\");
	} else {
		// Display data
		sprintf(sUnsigned, \"%u\", iResult);
		if (bByteData) {
			setValue(sControlName + \"Dec\", \"text\", sUnsigned);
			sprintf(sHex, \"%X\", iResult);
			setValue(sControlName + \"Hex\", \"text\", sHex);
		} else {
			setValue(sControlName, \"text\", sUnsigned);
		}
	}
	
	// Return to calling routine
	return;
}

string modConvertToHex(string sDecimalValue, unsigned uSize)
{
// Local Variables
// ---------------
	unsigned uDecimalValue;
	
	string sHex;	
	
// Executable Code
// ---------------
	// Convert value from string to unsigned
	uDecimalValue = sDecimalValue;
	
	// Print value to convert to hex of the required size
	sprintf(sHex, \"%0\" + uSize + \"X\", uDecimalValue);
	
	// Return to calling routine
	return (sHex);
}

string modConvertToDec(string sHexadecimalValue)
{
// Local Variables
// ---------------
	int iDec;
	
	string sDec;
	
// Executable Code
// ---------------
	// Convert value from string to integer (converting directly to unsigned doesn't work!)
	sscanf(sHexadecimalValue, \"%x\", iDec);
	
	// Convert value to string but with unsigned value
	sprintf(sDec, \"%u\", iDec);
	
	// Return to calling routine
	return (sDec);
}

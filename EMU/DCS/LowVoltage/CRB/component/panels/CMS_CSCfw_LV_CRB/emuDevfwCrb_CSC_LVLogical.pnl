  dynAppend(params_for_trend, \"$iMinYScaleC:0\");
  dynAppend(params_for_trend, \"$iMaxYScaleC:8\");
  dynAppend(params_for_trend, \"$channelNumber:\"+s_ch);
}
//===========================================================
void dmb_t_init()
{
  int i, index;
  dyn_int off_channels;
 
  for(i=1;i<=dynlen(chamber_fsms);i++)
   {
    dpGet(chamber_fsms[i]+\".off_channels\",off_channels);
    if(dynContains(off_channels,7))
     {
      setValue(\"t_\"+i,\"enabled\",false);
      setValue(\"te_\"+i,\"backCol\",\"{202,202,202}\");      
     }
    else
     {
      setValue(\"t_\"+i,\"enabled\",true);
      setValue(\"te_\"+i,\"backCol\",\"{0,153,153}\");        
     } 
   }
}
//===========================================================
void dmb_t_exclude()
{  
  string box_name,state;
  dyn_string ds1, ds2;
  int chn, chn_base0,i,index;
  dyn_int off_channels;
  bool isEnabled;
  getValue(\"\",\"name\",box_name);
  getValue(\"\",\"state\",0,isEnabled);  
  ds1=strsplit(box_name,\"_\");  
  chn=ds1[2];  
  if(isEnabled)
   {  
    dpGet(chamber_fsms[chn]+\".off_channels\",off_channels);
    if((index=dynContains(off_channels,7)))dynRemove(off_channels,index);
    dpSet(chamber_fsms[chn]+\".off_channels\",off_channels);
    setValue(\"t_\"+chn,\"enabled\",true);
    setValue(\"\",\"backCol\",\"{0,153,153}\");  
   }  
  else
   {
    dpGet(chamber_fsms[chn]+\".off_channels\",off_channels);
    if(!(index=dynContains(off_channels,7)))dynAppend(off_channels,7);
    dpSet(chamber_fsms[chn]+\".off_channels\",off_channels);
    setValue(\"t_\"+chn,\"enabled\",false);
    setValue(\"\",\"backCol\",\"{202,202,202}\");  
   }
}
//===========================================================
void main_exclude()
{
  string box_name,state;
  dyn_string test_dyn,test_dyn2,dp_frag,text_names;
  int chn, chn_base0,i;
  bool isEnabled;

  getValue(\"\",\"name\",box_name);
  getValue(\"\",\"state\",0,isEnabled);

  if(box_name==\"ch01\"){text_names=makeDynString(\"33_1\",\"34_1\",\"50_1\",\"ch01_t\");}
  else if(box_name==\"ch02\"){text_names=makeDynString(\"33_2\",\"34_2\",\"50_2\",\"ch02_t\");}
  else if(box_name==\"ch03\"){text_names=makeDynString(\"33_3\",\"34_3\",\"50_3\",\"ch03_t\");}
  else if(box_name==\"ch04\"){text_names=makeDynString(\"33_4\",\"34_4\",\"50_4\",\"ch04_t\");}
  else if(box_name==\"ch05\"){text_names=makeDynString(\"33_5\",\"34_5\",\"50_5\",\"ch05_t\");}
  else if(box_name==\"ch06\"){text_names=makeDynString(\"33_6\",\"34_6\",\"50_6\",\"ch06_t\");}
  else if(box_name==\"ch07\"){text_names=makeDynString(\"33_7\",\"34_7\",\"50_7\",\"ch07_t\");}
  else if(box_name==\"ch08\"){text_names=makeDynString(\"33_8\",\"34_8\",\"50_8\",\"ch08_t\");}
  else if(box_name==\"ch09\"){text_names=makeDynString(\"33_9\",\"34_9\",\"50_9\",\"ch09_t\");}
  else if(box_name==\"ccb\"){text_names=makeDynString(\"ccb_33\",\"ccb_50\",\"ccb_t\");}
  else if(box_name==\"cc\"){text_names=makeDynString(\"cc_33\",\"cc_50\",\"cc_t\");}
  else if(box_name==\"1.5\"){text_names=makeDynString(\"15_1\",\"15_2\",\"1.5_t\");}

  if(isEnabled)
   {
     state=\"include \"+box_name;
     for(i=1;i<=dynlen(text_names);i++)setValue(text_names[i],\"enabled\",true);
     setValue(\"\",\"backCol\",\"{0,153,153}\");  
   }  
  else
   {
     state=\"exclude \"+box_name;
     for(i=1;i<=dynlen(text_names);i++)setValue(text_names[i],\"enabled\",false);
     setValue(\"\",\"backCol\",\"{202,202,202}\");  
   }

  test_dyn=strsplit(state,\" \");
  test_dyn2=strsplit(test_dyn[2],\"h\");
  if(dynlen(test_dyn2)>=2)
   {
     chn=test_dyn2[2];
     chn_base0=chn-1;
     dp_frag=makeDynString(\"ai_\"+CSC_fwCAN1_g_LIST_5V[chn],
                           \"ai_\"+CSC_fwCAN1_g_LIST_3V1[chn],
                           \"ai_\"+CSC_fwCAN1_g_LIST_3V2[chn]);
   }
  if(test_dyn[2]==\"ccb\")
   {
     test_dyn2[2]=\"10\";
     dp_frag=makeDynString(\"ai_\"+CSC_fwCAN1_g_LIST_MPC[1],\"ai_\"+CSC_fwCAN1_g_LIST_MPC[2]);
   }
  else if(test_dyn[2]==\"cc\")
   {
     test_dyn2[2]=\"11\";
     dp_frag=makeDynString(\"ai_\"+CSC_fwCAN1_g_LIST_CC[1],\"ai_\"+CSC_fwCAN1_g_LIST_CC[2]);
   }
  else if(test_dyn[2]==\"1.5\")
   {
     test_dyn2[2]=\"12\";
     dp_frag=makeDynString(\"ai_\"+CSC_fwCAN1_g_LIST_15V[1],\"ai_\"+CSC_fwCAN1_g_LIST_15V[2]);
   }

  chn=test_dyn2[2];
  if(chn<1 || chn>12)
    {
      mudcsLvDebug(\"wrong format\\n righ format is:\\nexclude ch03\");
      return;
    }
  if(strpos(state,\"exclude\")>=0)exclude(1,test_dyn2[2],dp_frag);
  else if(strpos(state,\"include\")>=0)exclude(0,test_dyn2[2],dp_frag);
  else 
   {
    mudcsLvDebug(\"wrong format\\n righ format is:\\nexclude ch03\");
    return;
   }
}
//==================================
void exclude(int isexclude, string chn_text, dyn_string dp_frag)
{
  int chn=chn_text;
  dyn_int off_channels;
  int pos;
  dpGet(DpName+\".off_channels\",off_channels);
  if(isexclude)
   {
    if(dynContains(off_channels,chn)==0)dynAppend(off_channels,chn);
   }
  else
   {
    pos=dynContains(off_channels,chn);
    if(pos)dynRemove(off_channels,pos);
   }
  dpSetWait(DpName+\".off_channels\",off_channels);
  alert_reconfig(dp_frag, isexclude);   
}
//====================================
void alert_reconfig(dyn_string dp_frag, int isexclude)
{
  bool configExists,isActive;
  dyn_string alertPanelParameters,alertTexts,ilist, ilist_new,exceptionInfo;
  string alertHelp,alertPanel;
  int i,j,pos,current_status;
  dyn_int dplist;
  dyn_string dplist_text, ds1;
  dpGet(DpName+\".status\",current_status);
  fwAlertConfig_getSummary(DpName+\".\",configExists,alertTexts,
                           ilist,alertPanel,alertPanelParameters,
                           alertHelp,isActive,exceptionInfo);
  for(i=1;i<=dynlen(ilist);i++)
   { 
    for(j=1;j<=dynlen(dp_frag);j++)
     {
      if(strpos(ilist[i],dp_frag[j])>=0)
       {
         dynAppend(dplist,i);
       } // j
     }
   }
  ilist_new=ilist;
  if(isexclude)
   {
    dynClear(ilist);
    for(i=1;i<=dynlen(ilist_new);i++)
     {
      if(dynContains(dplist,i) == 0)dynAppend(ilist,ilist_new[i]);
     } 
   }
  else
   {
    for(j=1;j<=dynlen(dp_frag);j++)
     { 
      ds1=dpNames(\"*:*ELMB_\"+pnl_coord+\"*\"+dp_frag[j],\"FwElmbAi\");
      dynAppend(dplist_text,ds1); 
     } // for j   
    for(i=1;i<=dynlen(dplist_text);i++)
     {
      dynAppend(ilist,dplist_text[i]+\".value\"); 
     }
   }
  for(i=1;i<=dynlen(ilist);i++)
   {
    emuLvCRB_showDebug(bDebug,\"ilist[\"+i+\"]=\"+ilist[i]);
   } 						
  fwAlertConfig_createSummary(DpName+\".\",makeDynString(\"\",\"\"),ilist,
                              \"\", makeDynString(),\"\",exceptionInfo); 
  dpSet(mudcsLvAddSystem(\"ALERT_CONFIG_CHANGED_DP.\"),DpName);
}

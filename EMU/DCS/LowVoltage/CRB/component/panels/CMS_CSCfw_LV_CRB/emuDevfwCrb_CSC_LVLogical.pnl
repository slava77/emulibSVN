main()
{ 
  // ------ access control ------------
  if (isFunctionDefined(\"fwAccessControl_setupPanel\")) {
    initAcConstants();
    dyn_string ex;
    fwAccessControl_setupPanel(\"accessPrivilegesChangedCB\", ex);
    if (emu_checkException(ex)) { return; }
  }  
  //-----------------------------------   
  int i,j, index, index1; 
  dyn_string ds1,ds2,ds3;
  dyn_string error_list, split1;
  string s1,s2; 
  string bus_number, elmb_number, position;
  

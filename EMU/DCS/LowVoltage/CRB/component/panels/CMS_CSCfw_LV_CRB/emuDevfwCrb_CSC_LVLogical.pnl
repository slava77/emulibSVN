//====================================================
void get_dmb_temperatures(string dp, string value)
{
  int i,status;
  string test_string;
  dyn_string ds_alerts;
  dyn_float temps;
  dyn_int alert_channels; 
  
  dpGet(DpName+\".status\",status);    
  dynClear(chamber_fsms);
  mudcsCrbGetDmbTempAlerts(DpName, ds_alerts, alert_channels, chamber_fsms);
  emuLvCRB_showDebug(bDebug,\"ds_alerts:\"+ds_alerts);

  dmb_t_init();
  dpGet(ds_alerts, temps); 
  for(i=1;i<=dynlen(ds_alerts);i++)
    {   
     if(dynContains(alert_channels,i))setValue(\"t_\"+i,\"backCol\",\"FwAlarmErrorAck\");
     else setValue(\"t_\"+i,\"backCol\",\"White\");      
     sprintf(test_string,\"%8.2f\",temps[i]);
     setValue(\"t_\"+i,\"text\",test_string);
    }    
}
//============================================================================================
void update_errorlist(string dp, int state)
{
  dyn_string dsDpLists,error_list,ds1,ds2;
  string sAlert,s1;
  int index,index1;
  if(state == -1)
   {
    dpGet(DpName+\".:_alert_hdl.._dp_list\",dsDpLists);
    for(int i=1;i<=dynlen(dsDpLists);i++)
     {
      dpGet(dsDpLists[i]+\":_alert_hdl.._act_state_color\",sAlert);
      if(strpos(sAlert,\"Error\")<0)continue; 
      ds1=strsplit(dsDpLists[i],\".\");
      if(ds1[dynlen(ds1)]==\"value\")
       {
        ds2=strsplit(ds1[dynlen(ds1)-1],\"_\");
        index=ds2[dynlen(ds2)];
        if(index1=dynContains(CSC_fwCAN1_g_LIST_5V,index))       s1=\"5V #\"+index1;
        else if(index1=dynContains(CSC_fwCAN1_g_LIST_3V1,index)) s1=\"3V(1) #\"+index1;
        else if(index1=dynContains(CSC_fwCAN1_g_LIST_3V2,index)) s1=\"3V(2) #\"+index1;
        else if(index1=dynContains(CSC_fwCAN1_g_LIST_15V,index)) s1=\"1.5V #\"+index1;
        else if(index1=dynContains(CSC_fwCAN1_g_LIST_CC,index))  s1=\"CC #\"+index1;
        else if(index1=dynContains(CSC_fwCAN1_g_LIST_MPC,index)) s1=\"MPC #\"+index1;
        else if(index1=dynContains(CSC_fwCAN1_g_LIST_MAIN,index))s1=\"MAIN #\"+index1;                      
        dynAppend(error_list,s1);
       }
      else 
        dynAppend(error_list,ds1[dynlen(ds1)]);
      }
   } // for
  else
   { 
     error_list = makeDynString(\"\");
   }   
  setValue(\"error_list\",\"deleteAllItems\");
  setValue(\"error_list\",\"items\",error_list);
  setValue(\"error_list\",\"selectedPos\",1);  
}

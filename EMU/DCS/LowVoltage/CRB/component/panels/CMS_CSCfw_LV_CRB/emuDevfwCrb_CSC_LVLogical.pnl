void status_monitor(string dp, int status)
{
  dyn_string ds=makeDynString(\"33_1\",\"33_2\",\"33_3\",\"33_4\",\"33_5\",\"33_6\",\"33_7\",\"33_8\",\"33_9\",
                              \"34_1\",\"34_2\",\"34_3\",\"34_4\",\"34_5\",\"34_6\",\"34_7\",\"34_8\",\"34_9\",
                              \"50_1\",\"50_2\",\"50_3\",\"50_4\",\"50_5\",\"50_6\",\"50_7\",\"50_8\",\"50_9\",
                              \"cc_33\",\"cc_50\",\"ccb_33\",\"ccb_50\",\"15_1\",\"15_2\");
  if(status ==0 )
   {
     setValue(\"enable_disable\",\"text\",\"TO SWITCH ON\");
     setValue(\"enable_disable\",\"backCol\",\"{0,153,153}\");

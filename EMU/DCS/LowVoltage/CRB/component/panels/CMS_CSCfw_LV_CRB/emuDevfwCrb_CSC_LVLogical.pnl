void init_dp_switch()
{   
  if($sDpName != \"\")
   {   // the panel is called from logical view
     string sDpName = $sDpName;
     dyn_string ds1 = strsplit(sDpName,\":\");
     dyn_string ds2 = strsplit(ds1[2],\"/\");
     DpName=ds1[1]+\":CRB/\"+ds2[dynlen(ds2)];
   }  
  dyn_string dps;  
  dpGet(DpName+\".:_alert_hdl.._dp_list\",dps);
  if(dynlen(dps) <= 0)
   {
    DebugTN(DpName+\".:_alert_hdl.._active is not configured\");  
   }
}

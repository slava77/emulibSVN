{
  int i;
  string s1=\" position:\";
  dyn_string ds1,ds2,ds3; 
  dpGet(mudcsLvAddSystem(\"db_pcmb\")+\".list\", ds1);
  dpGet(mudcsLvAddSystem(\"db_pcmb_geographic\")+\".list\", ds2);

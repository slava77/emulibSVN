  else
   {
    for(int i=1;i<=dynlen(ds);i++)setValue(ds[i],\"enabled\",TRUE);
    setValue(\"enable_disable\",\"text\",\"TO SWITCH OFF\");
    setValue(\"enable_disable\",\"backCol\",\"{255,0,0}\");
   }

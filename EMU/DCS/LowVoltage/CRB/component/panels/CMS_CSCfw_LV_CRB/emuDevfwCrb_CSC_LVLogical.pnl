void init_name()
{
  //---to adapt for $sDpName in logical view---------
  string sDpName = $sDpName;
  string sDpNameNS = dpSubStr(sDpName, DPSUB_DP);
  dyn_string ds1 = strsplit(sDpNameNS,\"/\");    
  string node_conv=mudcsLvRemoveSystem(ds1[1]);  
  string obj_conv=mudcsLvRemoveSystem(\"CRB/\"+ds1[dynlen(ds1)]);
  //-------------------------------------------------------
  string fsm_node=node_conv+\"|\"+obj_conv;
  emuLvCRB_showDebug(bDebug,\"node_conv=\"+node_conv);
  emuLvCRB_showDebug(bDebug,\"obj_conv=\"+obj_conv);
  string get_label;
  dpGet(mudcsLvAddSystem(fsm_node)+\".ui.label\",get_label);
  emuLvCRB_showDebug(bDebug,\"label=\"+get_label);
  setValue(\"fsm_label\",\"text\",get_label);
  dyn_string split_get_label=strsplit(get_label,\" \");
  CAN_ID=split_get_label[dynlen(split_get_label)];
  emuLvCRB_showDebug(bDebug,\"CAN_ID=\"+CAN_ID);  
}

  emuLvCRB_initalizeParam(); //get system name where the CRB component has been installed 
  init_name();
  init_dp_switch();
  mudcsCrbInitDelays(1);
  emuLvCRB_showDebug(bDebug,\"DpName=\"+DpName);
  dpConnect(\"update_status\",false,DpName+\".status\");
  dpConnect(\"get_dmb_temperatures\",true,DpName+\".error_status\"); //create error because no middle layer project--xy

  bus_number=get_bus_number(DpName, elmb_number);
  position=get_geographic(DpName);
  setValue(\"bus_number\",\"text\",\"BUS#\"+bus_number+\"(count from 0)\"+position+\": \"+elmb_number);

  string local_sys = fwFsm_getSystemName();
  if(strpos(local_sys,\"CSC\")>=0 || strpos(local_sys,\"csc\")>=0 || strpos(local_sys,\"dcspcS2G19\")>=0 
   || strpos(local_sys,\"EMU\")>=0 || strpos(local_sys,\"emu\")>=0 || (local_sys+\":\"== gSystemNameCRB))
       is_local_sys=1;
  else 
       is_local_sys=0;
  if(is_local_sys)
     {
       setValue(\"enable_disable\",\"enabled\",TRUE); 
       for(i=1;i<=12;i++)
         {
           setValue(\"on\"+i,\"enabled\",TRUE);
           setValue(\"off\"+i,\"enabled\",TRUE);
         }
     }
//-------------------check CAN connection -------------------------
  dpConnect(\"update_connection\",DpName+\".status\");

  dyn_int off_channels;
  string isEnabled;
  dyn_string boxes_disabled;
  dpGet(DpName+\".off_channels\",off_channels);
  for(i=1;i<=dynlen(off_channels);i++)
    { 
      if(off_channels[i] >=1 && off_channels[i] <=9)
       {
         boxes_disabled=makeDynString(\"ch0\"+off_channels[i]+\"_t\",\"33_\"+off_channels[i],
                                      \"34_\"+off_channels[i],\"50\"+off_channels[i]);
         for(j=1;j<=dynlen(boxes_disabled);j++)setValue(boxes_disabled[j],\"enabled\",false);
         setValue(\"ch0\"+off_channels[i],\"backCol\",\"{202,202,202}\");
         setValue(\"ch0\"+off_channels[i],\"state\",0,false);                      
       }
      else if(off_channels[i]==10)
       {
         boxes_disabled=makeDynString(\"ccb\"+\"_t\",\"ccb_33\",\"ccb_50\");
         for(j=1;j<=dynlen(boxes_disabled);j++)setValue(boxes_disabled[j],\"enabled\",false); 
         setValue(\"ccb\",\"backCol\",\"{202,202,202}\");    
         setValue(\"ccb\",\"state\",0,false);                    
       }
      else if(off_channels[i]==11)
       {
         boxes_disabled=makeDynString(\"cc\"+\"_t\",\"cc_33\",\"cc_50\");
         for(j=1;j<=dynlen(boxes_disabled);j++)setValue(boxes_disabled[j],\"enabled\",false);
         setValue(\"cc\",\"backCol\",\"{202,202,202}\"); 
         setValue(\"cc\",\"state\",0,false);               
       }
      else if(off_channels[i]==12)
       {
         boxes_disabled=makeDynString(\"1.5\"+\"_t\",\"15_1\",\"15_2\");
         for(j=1;j<=dynlen(boxes_disabled);j++)setValue(boxes_disabled[j],\"enabled\",false); 
         setValue(\"1.5\",\"backCol\",\"{202,202,202}\");   
         setValue(\"1.5\",\"state\",0,false);                     
       }
    } // for i
  DpNameFsm2=mudcsLvRemoveSystem(DpName);
  split1=strsplit(DpNameFsm2,\"/\");
  DomainName=CSC_fwCAN1_g_csc_part;
  isEnabled=fwFsmUi_getDUMode(DomainName,DpNameFsm2);
  emuLvCRB_showDebug(bDebug,\"DomainName=\"+DomainName);
  emuLvCRB_showDebug(bDebug,\"DpNameFsm2=\"+DpNameFsm2);
  if(isEnabled==\"Disabled\")
   {
     setValue(\"isDeviceEnabled\",\"visible\",true);
     setValue(\"isDeviceEnabledCheck\",\"backCol\",\"{202,202,202}\");
     setValue(\"isDeviceEnabledCheck\",\"state\",0,0);    
   }
  else 
   {
     setValue(\"isDeviceEnabled\",\"visible\",false);
     setValue(\"isDeviceEnabledCheck\",\"backCol\",\"{0,153,153}\");
     setValue(\"isDeviceEnabledCheck\",\"state\",0,1);    
   }
  g_dps2=dpNames(CSC_fwCAN1_g_SYSTEM_NAME+\":*\"+DpNameFsm2,\"_FwFsmDevice\");
  dpConnect(\"update_enabled\",g_dps2[1]+\".mode.enabled\");
  //-------------------------------------------------------
  dpGet(DpName+\".coord\",pnl_coord);  
  ds1=dpNames(CSC_fwCAN1_g_SYSTEM_NAME+\":\"+\"*ELMB_\"+pnl_coord+\"*\",\"FwElmbAi\");
  pnl_channel_prefix=substr(ds1[1],0,strpos(ds1[1],\"/ai_\")+4);
  dpConnect(\"status_monitor\",DpName+\".status\");
  // add a dpconnect for update error status-----
  dpConnect(\"update_errorlist\",DpName+\".status\");
  
  periodic_update_monitor();
}

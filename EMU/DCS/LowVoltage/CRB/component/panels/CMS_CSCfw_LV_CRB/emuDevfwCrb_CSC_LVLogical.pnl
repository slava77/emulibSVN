//============================================================   
void update_connection(string DpName,int iStatus)
{
  int iErrorStatus;
  string sDpName = substr(DpName,0,strpos(DpName,\".\"));//remove .status from DpName
  dpGet(sDpName+\".error_status\",iErrorStatus); 
  if(iStatus == -1)
   { 
    if(iErrorStatus>0 && iErrorStatus<8000) 
      setValue(\"NOT_CONNECTED\",\"visible\",TRUE);
   } 
  else  setValue(\"NOT_CONNECTED\",\"visible\",FALSE);
}

//bool isGeneralSetting;

float t_min, t_max;
float h_min, h_max;
float p_min, p_max;


//isGeneralSetting = true;
//if(strpos($sDpName,device_type) >= 0 )isGeneralSetting = false;


limits_s = makeDynString();

/*

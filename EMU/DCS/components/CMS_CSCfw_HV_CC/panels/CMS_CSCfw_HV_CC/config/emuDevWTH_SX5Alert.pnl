*/

getValue(\"t_min\",\"text\",t_min);
getValue(\"h_min\",\"text\",h_min);
getValue(\"p_min\",\"text\",p_min);

getValue(\"t_max\",\"text\",t_max);
getValue(\"h_max\",\"text\",h_max);
getValue(\"p_max\",\"text\",p_max);


value=\".data.TEMPERATURE.sensor1|\"
         +\".data.TEMPERATURE.sensor2|\"
         +\".data.TEMPERATURE.sensor3|\"
         +\".data.TEMPERATURE.sensor4|\"
         +\".data.HUMIDITY.sensor1|\"
         +\".data.HUMIDITY.sensor2|\"

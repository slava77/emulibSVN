main()
{
	bool secondParentRequired, done = FALSE;
	int i, length;
	string dpName, dpType, currentDpLabel, newDpLabel, operationPanel, configPanel, parentDp, userLabel;
	dyn_string dpTypeList, exceptionInfo, configPanelList;


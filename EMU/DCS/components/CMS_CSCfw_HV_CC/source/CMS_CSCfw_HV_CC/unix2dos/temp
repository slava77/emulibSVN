//=================================================================" 0
 E E E E 1 -1 -1 0  420 39
""0  1
E "wth_set(bool isSet, string device_type)
{

dyn_string limits_s;
string value;
string s1,s2;
//bool isGeneralSetting;

float t_min, t_max;
float h_min, h_max;
float p_min, p_max;


//isGeneralSetting = true;
//if(strpos($sDpName,device_type) >= 0 )isGeneralSetting = false;


limits_s = makeDynString();


t_min=0;
t_max=45;
h_min=0;
h_max=70;
p_min=600;
p_max=1000;


value=\".data.TEMPERATURE.sensor1|\"
         +\".data.TEMPERATURE.sensor2|\"
         +\".data.TEMPERATURE.sensor3|\"
         +\".data.TEMPERATURE.sensor4|\"
         +\".data.HUMIDITY.sensor1|\"
         +\".data.HUMIDITY.sensor2|\"
         +\".data.HUMIDITY.sensor3|\"
         +\".data.PRESSURE.sensor1\";


//if(v18_d == 0 || v33_d == 0 || v55_d == 0 || v56_d == 0){
//  mudcsDebug(\"Interval should be > 0\");
//  return;
//}
 for(int i=1;i<=4;i++){
s1=t_min;
s2=t_max;
dynAppend(limits_s,s1+\"|\"+s2);
 }

 for(int i=1;i<=3;i++){
s1=h_min;
s2=h_max;
dynAppend(limits_s,s1+\"|\"+s2);
 }

 for(int i=1;i<=1;i++){
  s1=p_min;
  s2=p_max;
  dynAppend(limits_s,s1+\"|\"+s2);
 }


mudcsLookAndAlertArchiveConfig(true, 
false, isSet, device_type, value, limits_s, $sDpName, \"\",0,0,0.,0.);



if(dynlen(line) != 4 && test_string != \"\"){mudcsDebug(\"wrong format\");return;}

sscanf(line[2],\"%d\",setn);
//if(setn < 1 || setn > 24) {mudcsDebug(\"wrong format\");return;}


if((current_w_number==4 || current_w_number==5) && current_r_number==1){ // i.e. ME 1/1

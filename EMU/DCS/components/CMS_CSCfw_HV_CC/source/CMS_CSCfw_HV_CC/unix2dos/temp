""0  1
E "alnm_set(bool isSet, string device_type)
{

dyn_string limits_s;
string value;
string s1,s2;


// s1 is OK
// s2 is ALARM


limits_s = makeDynString();


value=\".data.general_status\";



s1=\"0\";
s2=\"1000000\";
dynAppend(limits_s,s1+\"|\"+s2);



mudcsLookAndAlertArchiveConfig(true, 
false, isSet, device_type, value, limits_s, $sDpName, \"\",0,0,0.,0.);


}


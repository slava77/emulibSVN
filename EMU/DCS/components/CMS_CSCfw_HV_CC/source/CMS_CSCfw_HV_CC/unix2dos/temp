	//stop busy bar
	fwBusyBarButton_std_stopBusy(exceptionInfo);

	//if problems occured, set progress text to show failure and display exceptions then return
	if(dynlen(exceptionInfo)>0)
	{
		resultCreation.text() = \"creating \"+dpName+\" failed.\";

//=================================================================" 0
 E E E E 1 -1 -1 0  420 39
""0  1
E "mrtn_set_d(bool isSet, string device_type)
{

int i;

dyn_string limits_s;
string value;
//string s1,s2;

limits_s = makeDynString();

// s1 is OK
// s2 is ALARM

dyn_string s1, s2;

s1[1]=\"1\";
s2[1]=\"2\";

s1[2]=\"0\";
s2[2]=\"1\";

s1[3]=\"1\";
s2[3]=\"2\";

s1[4]=\"0\";
s2[4]=\"1\";

s1[5]=\"0\";
s2[5]=\"1\";

s1[6]=\"0\";
s2[6]=\"1\";



for(i=1;i<=6;i++){

value=value+\".data.crate.item\"+i+\"|\";
 
 dynAppend(limits_s,s1[i]+\"|\"+s2[i]);

}




mudcsLookAndAlertArchiveConfig(true, 
false, isSet, device_type, value, limits_s, $sDpName, \"\",0,0,0.,0.);




}


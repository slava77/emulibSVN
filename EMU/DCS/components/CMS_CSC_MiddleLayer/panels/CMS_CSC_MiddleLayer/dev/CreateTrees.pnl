"#uses \"fwConfigurationDB/fwConfigurationDB.ctl\"
#uses \"fwConfigurationDB/fwConfigurationDB_DeviceConfiguration.ctl\"

const string CONFDB_PREFIX = \"MiddleLayer/\";

main()
{
  dyn_string ex;
  fwConfigurationDB_checkInit(ex);
  if (emu_checkException(ex)) { return; }
  
  dyn_string devices;
  
  if (chkSaveHv.state(0)) {
    // *** HV ***
    emu_dynAppend(devices, getDevicesFromHardwareHierarchy(getSystemName() + \"CscHighVoltage\", ex));
    if (emu_checkException(ex)) { return; }
    emu_dynAppend(devices, getDevicesFromHardwareHierarchy(getSystemName() + \"DimConfig\", ex, \"*HV*\"));
    if (emu_checkException(ex)) { return; }
    emu_dynAppend(devices, dpNames(\"*HV*\", \"CscDimCommand\"));
  
    saveConfiguration(devices, \"HV\", editVersion.text, ex, chkJustPrint.state(0));
    if (emu_checkException(ex)) { return; }

    // *** HV LV ***
    dynClear(devices);
    emu_dynAppend(devices, getDevicesFromHardwareHierarchy(getSystemName() + \"Wiener\", ex, \"*HV_LV_*\"));
    if (emu_checkException(ex)) { return; }

    saveConfiguration(devices, \"HV_LV\", editVersion.text, ex, chkJustPrint.state(0));
    if (emu_checkException(ex)) { return; }
  }
  
  if (chkSaveX2P.state(0)) {
    // *** X2P ***
    dynClear(devices);
    emu_dynAppend(devices, getDevicesFromHardwareHierarchy(getSystemName() + \"CscLowVoltage\", ex));
    if (emu_checkException(ex)) { return; }
    emu_dynAppend(devices, getDevicesFromHardwareHierarchy(getSystemName() + \"CscTemperature\", ex));
    if (emu_checkException(ex)) { return; }
    emu_dynAppend(devices, getDevicesFromHardwareHierarchy(getSystemName() + \"DimConfig\", ex, \"*X2P_[PM]*\"));
    if (emu_checkException(ex)) { return; }
    emu_dynAppend(devices, dpNames(\"*X2P*\", \"CscDimCommand\"));
  
    saveConfiguration(devices, \"X2P\", editVersion.text, ex, chkJustPrint.state(0));
    if (emu_checkException(ex)) { return; }
  
    // *** X2P FED ***
    dynClear(devices);
    emu_dynAppend(devices, getDevicesFromHardwareHierarchy(getSystemName() + \"CscFed\", ex));
    if (emu_checkException(ex)) { return; }
    emu_dynAppend(devices, getDevicesFromHardwareHierarchy(getSystemName() + \"DimConfig\", ex, \"*X2P_FED*\"));
    if (emu_checkException(ex)) { return; }
  
    saveConfiguration(devices, \"X2P_FED\", editVersion.text, ex, chkJustPrint.state(0));
    if (emu_checkException(ex)) { return; }
  }
  
  emu_info(\"DONE saving devices to conf DB as version \" + editVersion.text);
}

/**
  * @return devices from the specified node of the hardware hierarchy (also optionally matching a given pattern)
  */
dyn_string getDevicesFromHardwareHierarchy(string rootNode, dyn_string &ex, string pattern = \"\") {
  dyn_string deviceList;
  fwConfigurationDB_getHierarchyFromPVSS(rootNode, fwDevice_HARDWARE, deviceList, ex);
  if (emu_checkException(ex)) { return; }

  dyn_string ret;
  if (pattern != \"\") {
    for (int i=1; i <= dynlen(deviceList); i++) {
      string device = deviceList[i];
      if (patternMatch(pattern, device)) {
        dynAppend(ret, device);
      }
    }
  } else {
    ret = deviceList;
  }
  
  return ret;
}

/**
  * Saves given devices to configuration DB (if justPrint=true, it won't actually save the devices, but just print the list instead)
  */
void saveConfiguration(dyn_string devices, string name, string version, dyn_string &exceptionInfo, bool justPrint = FALSE) {
  string confName = CONFDB_PREFIX + name + \"_v\" + version;
  emu_info(\"Saving these devices to conf DB with name \" + confName);
  emu_info(devices);
  if (!justPrint) {
    fwConfigurationDB_saveDeviceConfiguration(devices, confName, fwDevice_HARDWARE, exceptionInfo);
    if (emu_checkException(exceptionInfo)) { return; }
  }
}" 0

"main()
{
  waitForEmuUiInitialize();
  drawMaratons();
  drawLines();
}

void drawMaratons() {
  int maratonWidth = 100;
  int maratonHeight = 50;
  
  int posX, posY;
  getValue(\"\", \"position\", posX, posY);   // get the position of the frame
  posX += 20;
  posY += 20;
  
  for (int id = 1; id <= 2; id++) {
    string sideStr = (id == 1) ? \"M\" : \"P\";
    
    string deviceParamsDollar = \"$deviceParams:\" + \"side=\" + sideStr +
                                                   \";station=1\" +
                                                   \";disk=1\" +
                                                   \";crateNum=1\" +
                                                   \";id=\" + id;
    string shapeParamsDollar = \"$shapeParams:\" + \"type=rectangle\" +
                                                 \";width=\" + maratonWidth +
                                                 \";height=\" + maratonHeight;
    
    //import the chamber panel (no changes in the panel orientation; position - [0,0])
    string refName = \"refMaraton_STEP_ID1\";  //reference name
    if (shapeExists(refName)) { refName += \"_1\"; }
    dynAppend(references, refName);
    addSymbol(myModuleName(), myPanelName(),
              EMUUI_OBJECTS_DIR + \"/refDevice.pnl\",
              refName,
              makeDynString(\"$caption:VALUE\", \"$deviceType:maraton\",
                            deviceParamsDollar, shapeParamsDollar, \"$group:\" + emuui_getSessionDp(\"maratonView\"),
                            \"$refName:\" + refName),
              posX, posY, 0, 1, 1);

    posX += maratonWidth;
  }
}

void drawLines() {
  int posX, posY;
  getValue(\"\", \"position\", posX, posY);   // get the position of the frame
  
  // 1st chamber lines in STEP area (channels 0 and 3)
  drawLine(posX + 20, posY + 60, 100, 90, 0, 1, 0);
  drawLine(posX + 20, posY + 140, 89, 0, 0, 1, 0);
  drawLine(posX - 50, posY + 79, 77, 90, 0, 1, 0);
  drawLine(posX - 50, posY + 80, 79, 0, 0, 1, 0);
  drawJunctionBoxTerminal(posX + 30, posY + 130, 0, 1, 0);

  // 2nd chamber lines in STEP area (channels 4 and 7)
  drawLine(posX + 40, posY + 60, 140, 90, 0, 1, 7);
  drawLine(posX + 40, posY + 170, 114, 0, 0, 1, 7);
  drawLine(posX - 50, posY + 169, 66, 90, 0, 1, 7);
  drawLine(posX - 50, posY + 220, 79, 0, 0, 1, 7);
  drawJunctionBoxTerminal(posX + 50, posY + 160, 0, 1, 7);

  // PCrate lines in STEP area (channels 1 and 5)
  drawLine(posX + 60, posY + 60, 349, 90, 0, 1, 1);
  drawJunctionBoxTerminal(posX + 70, posY + 190, 0, 1, 1);

  // 1st chamber lines in LTT area (channels 0 and 3)
  drawLine(posX + 190, posY + 60, 100, 90, 0, 2, 0);
  drawLine(posX + 270, posY + 140, 100, 0, 0, 2, 0);
  drawLine(posX + 270, posY + 89, 354, 90, 0, 2, 0);
  drawLine(posX + 330, posY + 90, 75, 0, 0, 2, 0);
  drawLine(posX + 330, posY + 230, 75, 0, 0, 2, 0);
  drawLine(posX + 330, posY + 370, 75, 0, 0, 2, 0);
  drawJunctionBoxTerminal(posX + 200, posY + 130, 0, 2, 0);

  // 2st chamber lines in LTT area (channels 4 and 7)
  drawLine(posX + 170, posY + 60, 140, 90, 0, 2, 4);
  drawLine(posX + 250, posY + 170, 100, 0, 0, 2, 4);
  drawLine(posX + 250, posY + 169, 778, 90, 0, 2, 4);

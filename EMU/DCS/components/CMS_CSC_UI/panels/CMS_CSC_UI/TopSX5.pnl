#uses \"CMS_CSC_UI/background_checks/emuui_backgroundChecksMain.ctl\"

// Majority library
#uses \"majority_treeCache/majorityLib.ctl\"

const int CHAMBER_OUTER_WIDTH = 120;
const int CHAMBER_INNER_WIDTH = 60;
const int CHAMBER_HEIGHT = 320;

dyn_string references;
dyn_string activeAlerts;

void waitForEmuUiInitialize() {
  while(!emuui_isInitialized()) { // wait for the emuui initialization
    delay(0, 200);
  }
}

void initAlerts() {
  // this basically returns all DPEs in the system since everybody actually appear to have a default alert handler
  dyn_string allAlertTypeDpes = dpNames(\"*.**:_alert_hdl.._type\");
  dyn_int alertTypes;
  dpGet(allAlertTypeDpes, alertTypes);
  dyn_string realAlertDpes;
  for (int i=1; i <= dynlen(allAlertTypeDpes); i++) {
    if (alertTypes[i] != 0) { // only those which have _alert_hdl.._type != 0 are real alert handlers - filter only those
      dynAppend(realAlertDpes, allAlertTypeDpes[i]);
    }
  }
  
  // send the alert state facts for all of the real alerts
  for (int i=1; i <= dynlen(realAlertDpes); i++) {
    handleAlert(realAlertDpes[i]);
  }
}

void updateAlertsCB(time t, int count, string alert) {
  startThread(\"handleAlert\", alert);
}

void handleAlert(string alert) {
  string alertConf = dpSubStr(alert, DPSUB_SYS_DP_EL_CONF);
  string alertDp = dpSubStr(alert, DPSUB_DP_EL);

  // determine state and text  
  int actState;
  string actText;
  anytype value;
  dpGet(alertConf + \".._act_state\", actState,
        alertConf + \".._act_text\", actText);
  
  if (actState == 0) {
    alertTable.deleteLine(1, \"dp\", alertDp);
  } else {
    alertTable.deleteLine(1, \"dp\", alertDp);
    alertTable.appendLine(\"dp\", alertDp, \"text\", actText);
  }
  
  if (alertTable.lineCount > 0) {
    alertTable.backCol = \"red\";
    alertIndicator.visible = true;
  } else {
    alertTable.backCol = \"\";
    alertIndicator.visible = false;
  }
}

void timestampUpdatedCB(dyn_string dps, dyn_time timestamps) {
  time lastTime = timestamps[1];
  for (int i=1; i <= dynlen(timestamps); i++) {
    if (timestamps[i] > lastTime) {
      lastTime = timestamps[i];
    }
  }
  txtTimeLastRead.text = formatTime(\"%Y.%m.%d %H:%M:%S\", lastTime);

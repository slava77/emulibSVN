void drawSensors(int posX, int posY, string side, int station, int ring, string strChamberNumber) {
  string chamberName = \"ME_\" + side + station + ring + \"_C\" + strChamberNumber;
  //LVDB
  drawLabel(posX - 43, posY + 3, \"LVDB:\", \"label_temperature_\" + chamberName + \"_LVDB\");
  drawTempSensor(posX, posY, side, station, ring, strChamberNumber, \"lvdb\", 1);
  //ALCT
  drawLabel(posX - 43, posY + 23, \"ALCT:\", \"label_temperature_\" + chamberName + \"_ALCT\");
  drawTempSensor(posX, posY + 20, side, station, ring, strChamberNumber, \"alct\", 1);
  // DCFEBs
  drawLabel(posX - 43, posY + 43, \"DCFEBs:\", \"label_temperature_\" + chamberName + \"_DCFEB\");
  for (int i=1; i <= 7; i++) {
    drawTempSensor(posX + 20 * (i-1), posY + 40, side, station, ring, strChamberNumber, \"dcfeb_fpga\", i);
  }
  //ODMB
  drawLabel(posX - 43, posY + 63, \"ODMB:\", \"label_temperature_\" + chamberName + \"_ODMB\");
  drawTempSensor(posX, posY + 60, side, station, ring, strChamberNumber, \"odmb\", 1);
  //OTMB
  drawLabel(posX - 43, posY + 83, \"OTMB:\", \"label_temperature_\" + chamberName + \"_OTMB\");
  drawTempSensor(posX, posY + 80, side, station, ring, strChamberNumber, \"otmb\", 1);

  // Input voltage
  drawLabel(posX + 150, posY + 33, \"7V digital:\", \"label_temperature_\" + chamberName + \"_OTMB\");
  drawVoltageSensor(posX + 200, posY + 30, side, station, ring, strChamberNumber, \"lvdb_digital\", 1);
  drawLabel(posX + 150, posY + 53, \"7V analog:\", \"label_temperature_\" + chamberName + \"_OTMB\");
  drawVoltageSensor(posX + 200, posY + 50, side, station, ring, strChamberNumber, \"lvdb_analog\", 1);
}

void drawTempSensor(int posX, int posY, string side, int station, int ring, string strChamberNumber, string boardType, int boardNumber) {
  string deviceParamsDollar = \"$deviceParams:\" + \"side=\" + side +
                                                 \";station=\" + station +
                                                 \";ring=\" + ring +
                                                 \";chamberNumber=\" + strChamberNumber +
                                                 \";boardType=\" + boardType +
                                                 \";boardNumber=\" + boardNumber;

  string shapeParamsDollar = \"$shapeParams:\" + \"type=rectangle\" +
                                             \";width=\" + 20 +
                                             \";height=\" + 20;
    
  //reference name
  string refName = \"refTempSensor_temp_ME_\" + side + station + ring + \"_C\" + strChamberNumber + \"_\" + boardType + \"_\" + boardNumber;
  if (shapeExists(refName)) { refName += \"_1\"; }
  dynAppend(references, refName);
  addSymbol(myModuleName(), myPanelName(),
            EMUUI_OBJECTS_DIR + \"/refDevice.pnl\",
            refName,
            makeDynString(\"$caption:VALUE\", \"$deviceType:chamber_\" + boardType,
                          deviceParamsDollar, shapeParamsDollar, \"$group:\" + emuui_getSessionDp(\"valueMonitor\"),
                          \"$refName:\" + refName),
            posX, posY, 0, 1, 1);  
}

void drawVoltageSensor(int posX, int posY, string side, int station, int ring, string strChamberNumber, string boardType, int boardNumber) {
  string deviceParamsDollar = \"$deviceParams:\" + \"side=\" + side +

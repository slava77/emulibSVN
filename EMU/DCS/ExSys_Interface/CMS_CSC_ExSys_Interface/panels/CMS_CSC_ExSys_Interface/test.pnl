"main()
{
  dyn_string ex;
  int factCollHdl = exsys_createFactCollection(ex);
  time t0;
  emu_debugFuncStart(\"create500DummyFacts\", t0);
  
  dyn_string paramNames = makeDynString(\"dummyParam1\", \"dummyParam2\", \"dummyParam3\");
  dyn_string paramValues = makeDynString(\"dummyValue1\", \"dummyValue2\", \"dummyValue3\");
  
  for (int i=0; i < 500; i++) {
    exsys_createFact(factCollHdl, \"dcsDummyFact\", t0, \"ME+1/1/\" + i, EXSYS_FACT_SEVERITY_DEBUG,
                     \"dummy fact #\" + i, paramNames, paramValues, ex);
    if (emu_checkException(ex)) { return; }
  }
  
  exsys_getFactCollection(factCollHdl, ex);
  if (emu_checkException(ex)) { return; }
  
  emu_debugFuncEnd(\"create500DummyFacts\", t0);
}" 0

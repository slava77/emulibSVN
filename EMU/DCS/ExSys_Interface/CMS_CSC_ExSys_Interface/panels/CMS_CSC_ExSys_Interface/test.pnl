    if (emu_checkException(ex)) { return; }
    
    if (i % 100 == 0) {
      emu_info(\"send10000Facts progress: \" + i + \"/10000\");
    }
  }
  
  emu_debugFuncEnd(\"send10000Facts\", t0);
}" 0

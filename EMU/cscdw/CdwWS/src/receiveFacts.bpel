<?xml version="1.0" encoding="UTF-8"?>
<process
    name="receiveFacts"
    targetNamespace="http://enterprise.netbeans.org/bpel/CdwBpel/receiveFacts"
    xmlns="http://docs.oasis-open.org/wsbpel/2.0/process/executable"
    xmlns:xsd="http://www.w3.org/2001/XMLSchema"
    xmlns:sxt="http://www.sun.com/wsbpel/2.0/process/executable/SUNExtension/Trace" 
    xmlns:sxed="http://www.sun.com/wsbpel/2.0/process/executable/SUNExtension/Editor"
    xmlns:sxat="http://www.sun.com/wsbpel/2.0/process/executable/SUNExtension/Attachment"
    xmlns:sxeh="http://www.sun.com/wsbpel/2.0/process/executable/SUNExtension/ErrorHandling"
    xmlns:tns="http://enterprise.netbeans.org/bpel/CdwBpel/receiveFacts" xmlns:sxed2="http://www.sun.com/wsbpel/2.0/process/executable/SUNExtension/Editor2" xmlns:ns0="http://www.cern.ch/cms/csc" xmlns:ns1="http://j2ee.netbeans.org/xsd/tableSchema">
    <import namespace="http://j2ee.netbeans.org/wsdl/CdwBpel/FactInput" location="FactInput.wsdl" importType="http://schemas.xmlsoap.org/wsdl/"/>
    <import namespace="http://www.cern.ch/cms/csc" location="fact.xsd" importType="http://www.w3.org/2001/XMLSchema"/>
    <import namespace="http://j2ee.netbeans.org/wsdl/FactTable" location="FactTable.wsdl" importType="http://schemas.xmlsoap.org/wsdl/"/>
    <partnerLinks>
        <partnerLink name="DBPartnerLink" xmlns:tns="http://j2ee.netbeans.org/wsdl/FactTable" partnerLinkType="tns:jdbcpartner" partnerRole="jdbcPortTypeRole"/>
        <partnerLink name="FactInputPartnerLink" xmlns:tns="http://j2ee.netbeans.org/wsdl/CdwBpel/FactInput" partnerLinkType="tns:FactInput" myRole="FactInputPortTypeRole"/>
    </partnerLinks>
    <variables>
        <variable name="FactInputOperationOut" xmlns:tns="http://j2ee.netbeans.org/wsdl/CdwBpel/FactInput" messageType="tns:FactInputOperationResponse"/>
        <variable name="FactInputOperationIn" xmlns:tns="http://j2ee.netbeans.org/wsdl/CdwBpel/FactInput" messageType="tns:FactInputOperationRequest"/>
    </variables>
    <sequence>
        <receive name="FactProviderReceive" createInstance="yes" partnerLink="FactInputPartnerLink" operation="FactInputOperation" xmlns:tns="http://j2ee.netbeans.org/wsdl/CdwBpel/FactInput" portType="tns:FactInputPortType" variable="FactInputOperationIn"/>
        <assign name="ResetResult">
            <copy>
                <from>0</from>
                <to variable="FactInputOperationOut" part="result"/>
            </copy>
        </assign>
        <forEach name="ForEachFact" parallel="no" counterName="ForEachFactCounter">
            <startCounterValue>1</startCounterValue>
            <finalCounterValue>count($FactInputOperationIn.collection)</finalCounterValue>
            <scope name="DBInputScope">
                <variables>
                    <variable name="InsertOut" xmlns:tns="http://j2ee.netbeans.org/wsdl/FactTable" messageType="tns:insertRetMsg"/>
                    <variable name="InsertIn" xmlns:tns="http://j2ee.netbeans.org/wsdl/FactTable" messageType="tns:inputMsg"/>
                    <variable name="currentFact" type="ns0:factType"/>
                </variables>
                <sequence name="Sequence1">
                    <assign name="AssignCurrentFact">
                        <copy>
                                <from>$FactInputOperationIn.collection/ns0:fact[position() = $ForEachFactCounter]</from>
                                    <to variable="currentFact"/>
                            </copy>
                    </assign>
                    <assign name="FactToDBFact">
                        <copy>
                            <from>number($currentFact/ns0:value)</from>
                            <to>$InsertIn.part/ns1:CDW_FACTS_Record/ns1:FCT_VALUE</to>
                        </copy>
                        <copy>
                            <from>number($currentFact/ns0:lowerThreshold)</from>
                            <to>$InsertIn.part/ns1:CDW_FACTS_Record/ns1:FCT_LOWER_THRESHOLD</to>
                        </copy>
                        <copy>
                            <from>number($currentFact/ns0:fraction)</from>
                            <to>$InsertIn.part/ns1:CDW_FACTS_Record/ns1:FCT_FRACTION</to>
                        </copy>
                        <copy>
                            <from>string($currentFact/ns0:status)</from>
                            <to>$InsertIn.part/ns1:CDW_FACTS_Record/ns1:FCT_STA_ID</to>
                        </copy>
                        <copy>
                            <from>string($currentFact/ns0:descr)</from>
                            <to>$InsertIn.part/ns1:CDW_FACTS_Record/ns1:FCT_DESCR</to>
                        </copy>
                        <copy>
                            <from>true()</from>
                            <to>$InsertIn.part/ns1:CDW_FACTS_Record/ns1:FCT_ID/@isNull</to>
                        </copy>
                        <copy>
                            <from>number($currentFact/ns0:upperThreshold)</from>
                            <to>$InsertIn.part/ns1:CDW_FACTS_Record/ns1:FCT_UPPER_THRESHOLD</to>
                        </copy>
                        <copy>
                            <from>string($currentFact/ns0:component)</from>
                            <to>$InsertIn.part/ns1:CDW_FACTS_Record/ns1:FCT_COMPONENT_ID</to>
                        </copy>
                        <copy>
                            <from>string($currentFact/ns0:severity)</from>
                            <to>$InsertIn.part/ns1:CDW_FACTS_Record/ns1:FCT_SEV_ID</to>
                        </copy>
                        <copy>
                            <from>string($currentFact/ns0:type)</from>
                            <to>$InsertIn.part/ns1:CDW_FACTS_Record/ns1:FCT_FTY_ID</to>
                        </copy>
                        <copy>
                            <from>string($FactInputOperationIn.collection/@source)</from>
                            <to>$InsertIn.part/ns1:CDW_FACTS_Record/ns1:FCT_SRC_ID</to>
                        </copy>
                    </assign>
                    <invoke name="DBInsert" partnerLink="DBPartnerLink" operation="insert" xmlns:tns="http://j2ee.netbeans.org/wsdl/FactTable" portType="tns:jdbcPortType" inputVariable="InsertIn" outputVariable="InsertOut"/>
                    <assign name="SumResult">
                        <copy>
                            <from>$FactInputOperationOut.result + $InsertOut.part</from>
                            <to variable="FactInputOperationOut" part="result"/>
                        </copy>
                    </assign>
                </sequence>
            </scope>
        </forEach>
        <reply name="FactProviderReply" partnerLink="FactInputPartnerLink" operation="FactInputOperation" xmlns:tns="http://j2ee.netbeans.org/wsdl/CdwBpel/FactInput" portType="tns:FactInputPortType" variable="FactInputOperationOut"/>
    </sequence>
</process>
